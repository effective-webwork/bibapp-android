<?xml version="1.0" encoding="utf-8"?>
<resources
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingTranslation">
    <!--
        Block orders if the listed keys contains a "b" character at the second position in a
        UNAPI picaxml request.
    -->
    <array name="block_order_types">
        <item>002@</item>
    </array>

    <!--
        If true, the detail view will not initially requests DAIA, but first check the FamURL
    -->
    <bool name="use_daia_sub_requests">false</bool>

    <!--
        If "use_daia_sub_requests" is true, these are the FamURL's for each configured catalog.
    -->
    <array name="fam_urls">
    </array>

    <!--
        The UnAPI URL for extended information
        %1 - PPN
        %2 - Format
    -->
    <string-array name="bibapp_unapi_local_urls">
        <item>http://unapi.k10plus.de/\?id=opac-de-27:ppn:%1$s&amp;format=%2$s</item>
    </string-array>
    <string name="bibapp_unapi_gvk_url">http://unapi.k10plus.de/\?id=gvk:ppn:%1$s&amp;format=%2$s</string>

    <!--
        URL for getting cover images
        %1 - ISBN
    -->
    <string name="bibapp_cover_url">http://ws.gbv.de/covers/\?id=%1$s&amp;format=img</string>

    <!--
        List of URLs to be shown as homepage links in the main navigation, one for each local
        catalog
    -->
    <string-array name="bibapp_homepage_urls">
        <item>http://www.thulb.uni-jena.de</item>
    </string-array>

    <!--
        URL for Data Privacy information
    -->
    <string name="bibapp_dataprivacy_url"></string>
</resources>